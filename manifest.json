{
  "name": "m3dium incog",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "opens paywalled medium links automatically in incognito mode",
  "author": "Mehedi Hassan",
  "homepage_url": "https://builtbymeh.com",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": [
      "src/bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://*/*"
      ],
      "js": [
        "src/inject/inject.js"
      ]
    }
  ]
}